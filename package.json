{
  "name": "next-client-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore --ext .js,ts,.tsx src",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|jsx)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "check-types": "tsc",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "validate": "yarn check-format && yarn lint && yarn build"
  },
  "dependencies": {
    "jest": "^26.2.2",
    "next": "9.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.10.4",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.0",
    "@types/jest": "26.0.9",
    "@types/node": "14.0.27",
    "@types/react": "16.9.44",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-testing-library": "^3.4.1",
    "husky": "4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.1.2",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "typescript": "3.9.7"
  }
}
